{
  "kind": "collectionType",
  "collectionName": "pedidos",
  "info": {
    "singularName": "pedido",
    "pluralName": "pedidos",
    "displayName": "Pedido"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "fecha": {
      "type": "datetime"
    },
    "estado": {
      "type": "enumeration",
      "enum": [
        "solicitado",
        "cancelado",
        "despachado",
        "entregado"
      ]
    },
    "total": {
      "type": "decimal"
    },
    "cliente": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "pedidos"
    },
    "items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::detalle-pedido.detalle-pedido",
      "mappedBy": "pedido"
    }
  }
}
